{
    "name": "nextgendashboard",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start:server": "node gen/server.min.js",
        "build:prod": "cross-env NODE_ENV=production webpack --profile --progress --colors --config app_modules/compiler/webpack/webpack.config.babel.js",
        "precommit": "lint-staged --config app_modules/hooks/husky/.lintstagedrc",
        "cosmics": "prettier --config app_modules/cosmics/prettier/.prettierrc --write \"app/**/*.{js,scss}\"",
        "lint:js": "eslint --config app_modules/linter/js-linter/.eslintrc app/**/*.js",
        "lint:style": "stylelint --config app_modules/linter/style-linter/.stylelintrc \"app/**/*.{scss}\"",
        "linter": "npm-run-all --parallel lint:js lint:style",
        "kill-node": "sh kill.node.sh"
    },
    "author": "Harish Singh Dhami <hsd.1989@gmail.com> (https://github.com/hdhami)",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "cross-env": "^5.2.0",
        "editorconfig": "^0.15.2",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.8.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "fs-extra": "^7.0.1",
        "husky": "^1.2.1",
        "jest": "^23.6.0",
        "lint-staged": "^8.0.4",
        "mini-css-extract-plugin": "^0.5.0",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "pa11y": "^5.1.0",
        "pa11y-ci": "^2.1.1",
        "prettier": "1.15.3",
        "stylelint": "^9.7.1",
        "uglifyjs-webpack-plugin": "^2.1.1",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.2.1",
        "webpack-merge": "^4.2.1"
